{
	"name": "apk-locator",
	"displayName": "Apk Locator",
	"description": "Easily find and open the folder containing your apk file.",
	"version": "0.0.1",
	"publisher": "itscap",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.locate_apk"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.locate_apk",
				"title": "Locate APK"
			}
		]
	},
	"scripts": {
		"build": "babel src -d lib",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.4",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	}
}

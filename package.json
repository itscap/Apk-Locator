{
	"name": "apk-locator",
	"displayName": "Apk Locator",
	"description": "Easily open in folder your apk",
	"version": "0.0.1",
	"publisher": "",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.open_apk_release_folder"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.open_apk_release_folder",
			"title": "Open apk release folder"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
